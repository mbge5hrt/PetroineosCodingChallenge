<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!-- ReportingLocation: Path to write reports -->
    <add key="ReportingLocation" value="C:\users\test" />

    <!-- ReportingInterval: Frequency of reporting in minutes -->
    <add key="ReportingInterval" value="1" />

    <!-- MaxRetries: Number of times to re-run the report on failure, -1 is indefinitely -->
    <add key="MaxRetries" value="5" />

    <!-- LogFilename: Filename to write log output -->
    <add key="LogFilename" value="C:\users\test\powerservice.log" />

    <!-- LogLevel: Level of logging required e.g. Debug, Info, Error -->
    <add key="LogLevel" value="Info" />
  </appSettings>
</configuration>